// mymath.h
#ifndef MYMATH_H
#define MYMATH_H

int add(int a, int b);

#endif

// mymath.c
#include "mymath.h"

int add(int a, int b) {
    return a + b;
}

#include <stdio.h>
#include "mymath.h"

int main() {
    int num1 = 22;
    int num2 = 33;
    int result = add(num1, num2);
    
    printf("%d + %d = %d\n", num1, num2, result);
    
    return 0;
}

